import struct

def convert_32bitword_to_float(a):
    sign = a[0]
    exp = a[1:9]
    mant = a[9:]
    
    mantissa = 0
    for i in range(23):
        mantissa += int(mant[i]) * 2**(-i-1)
    exponent = int(exp,2)  
    return (-1)**int(sign) * (1 + mantissa) * 2**(exponent - 127)

print(convert_32bitword_to_float("00110011111000110000011100000000"))
